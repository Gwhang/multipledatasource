192.168.92.11
CREATE TABLE `customer` (
  `uuid` varchar(36) NOT NULL,
  `customerNo` varchar(12) DEFAULT NULL COMMENT '客户编码',
  `customerName` varchar(255) DEFAULT NULL COMMENT '客户姓名',
  `customerType` varchar(5) DEFAULT NULL COMMENT '客户类型 01 个人客户 02 企业客户',
  `customerTypeName` varchar(255) DEFAULT NULL COMMENT '客户类型名称',
  `customerAddr` varchar(255) DEFAULT NULL COMMENT '用户地址',
  `customerTel` varchar(50) DEFAULT NULL COMMENT '客户手机号码',
  `customerIdType` varchar(5) DEFAULT NULL COMMENT '客户证件类型 01 身份证 02 军官证 03 港澳通行证 04 护照 05 其他证件',
  `customerIdTypeName` varchar(255) DEFAULT NULL COMMENT '客户证件类型名称',
  `IdNumber` varchar(50) DEFAULT NULL COMMENT '证件号码',
  `createDate` datetime DEFAULT NULL COMMENT '注册时间',
  PRIMARY KEY (`uuid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `store` (
  `uuid` varchar(36) NOT NULL,
  `customerNo` varchar(20) DEFAULT NULL COMMENT '客户编码',
  `storeCode` varchar(255) DEFAULT NULL COMMENT '注册门店编码',
  `storeName` varchar(255) DEFAULT NULL COMMENT '注册门店名称',
  `createDate` datetime DEFAULT NULL COMMENT '注册时间',
  PRIMARY KEY (`uuid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;


